package juego;

public class luchador extends personaje{

	protected boolean guantes, pechera;
	
	protected luchador(String nombre, String fisico1, String fisico2, String magico1, String magico2, String eficacia, boolean guantes, boolean pechera) {
		super(nombre, fisico1, fisico2, magico1, magico2, eficacia);
		this.guantes = false;
		this.pechera = false;
		}

	

	protected boolean isGuantes() {
		return guantes;
	}

	protected void setGuantes(boolean guantes) {
		this.guantes = guantes;
	}

	protected boolean isPechera() {
		return pechera;
	}

	protected void setPechera(boolean pechera) {
		this.pechera = pechera;
	}

}
