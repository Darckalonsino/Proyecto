package juego;

public class hechicero extends personaje{

	protected boolean sombrero, tunica;
	
		protected hechicero(String nombre, String fisico1, String fisico2, String magico1, String magico2, String eficacia, int dmg, boolean sombrero, boolean tunica) {
			super(nombre, fisico1, fisico2, magico1, magico2, eficacia, dmg);
			this.sombrero = false;
			this.tunica = false;
	}

		protected boolean isSombrero() {
			return sombrero;
		}

		protected void setSombrero(boolean sombrero) {
			this.sombrero = sombrero;
		}

		protected boolean isTunica() {
			return tunica;
		}

		protected void setTunica(boolean tunica) {
			this.tunica = tunica;
		}
		

}
